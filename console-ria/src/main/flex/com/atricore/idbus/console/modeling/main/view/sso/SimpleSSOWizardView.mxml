<Wizard
        xmlns="com.atricore.idbus.console.components.wizard.*"
        xmlns:mx="library://ns.adobe.com/flex/mx"
        xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:s="library://ns.adobe.com/flex/spark"
        xmlns:ssowizardsteps="com.atricore.idbus.console.modeling.main.view.sso.*"
        title="New Simple SSO Identity Appliance"
        showSummaryStep="true">

        <steps>
            <ssowizardsteps:IdentityApplianceStep stepId="appliance" jumpToStep="certificateStep"
                dataField="applianceData"/>
            <ssowizardsteps:ManageCertificateStep stepId="certificateStep" jumpToStep="authStep"
                dataField="certificateData"/>
            <ssowizardsteps:BasicAuthStep stepId="authStep" jumpToStep="spStep"
                dataField="authData"/>
            <ssowizardsteps:DbIdentitySourceStep stepId="databaseStep" jumpToStep="spStep"
                dataField="databaseData"/>
            <ssowizardsteps:LdapIdentitySourceStep stepId="ldapStep" jumpToStep="spStep"
                dataField="ldapData"/>
            <ssowizardsteps:XmlIdentitySourceStep stepId="xmlStep" jumpToStep="spStep"
                dataField="xmlData"/>
            <ssowizardsteps:ServiceProviderStep stepId="spStep"
                dataField="spData"/>
        </steps>
</Wizard>

