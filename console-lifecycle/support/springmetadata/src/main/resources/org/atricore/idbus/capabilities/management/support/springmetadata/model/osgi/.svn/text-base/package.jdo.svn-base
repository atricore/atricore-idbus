<jdo xmlns="http://java.sun.com/xml/ns/jdo/jdo"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://java.sun.com/xml/ns/jdo/jdo
        http://java.sun.com/xml/ns/jdo/jdo_2_0.xsd">

    <package name="org.atricore.idbus.capabilities.management.support.springmetadata.model.osgi">

        <class name="TbaseService">
            <inheritance strategy="new-table"/>
        </class>

        <class name="Service">
            <inheritance strategy="new-table"/>

            <field name="interfaces" dependent="true" default-fetch-group="true">
                <join delete-action="cascade"/>
            </field>

            <field name="registrationListeners" default-fetch-group="true">
                <collection dependent-element="true"/>
                <join delete-action="cascade"/>
            </field>

            <!-- TODO : Support field 'any' -->

        </class>

        <class name="TserviceRegistrationListener">
            <inheritance strategy="new-table"/>
            <!-- TODO : Support field 'any' -->
        </class>

        <class name="Tlistener">
            <inheritance strategy="new-table"/>
            <!-- TODO : Support field 'any' -->
        </class>

        <class name="Treference">
            <inheritance strategy="new-class"/>

            <field name="listeners" default-fetch-group="true">
                <collection dependent-element="true"/>
                <join delete-action="cascade"/>
            </field>
        </class>

        <class name="Reference">
            <inheritance strategy="new-class"/>
        </class>

        <class name="Bundle">
            <inheritance strategy="new-class"/>
        </class>

    </package>
</jdo>