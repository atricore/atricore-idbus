<process-definition name='SAMLR2 Encrypt Assertion attributes'>

    <description>Secure a SAMLR2 Assertion attributes</description>

    <start-state name="start">
        <transition to="samlr2EncryptAssertionAttrs" />
    </start-state>

    <state name="samlr2EncryptAssertionAttrs">
        <event type="node-enter">
            <action class="org.atricore.idbus.capabilities.samlr2.main.common.plans.actions.EncryptAssertionAttributesAction"
                    name="SAMLR2 Encrypt Assertion">
            </action>
        </event>
        <transition to="end" />
    </state>

    <end-state name="end"/>

</process-definition>