<process-definition name='SAMLR2 Sign Response'>

    <description>Sign SAMLR2 Response</description>

    <start-state name="start">
        <transition to="samlrSignResponse"/>
    </start-state>

    <state name="samlrSignResponse">
        <event type="node-enter">
            <action class="org.atricore.idbus.capabilities.samlr2.main.common.plans.actions.SignResponseAction"
                    name="SAMLR2 Sign Response">
            </action>
        </event>

        <transition to="end" />
    </state>

    <end-state name="end" />

</process-definition>