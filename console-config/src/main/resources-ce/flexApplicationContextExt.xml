<?xml version="1.0" encoding="utf-8"?>
<objects
        xmlns="http://www.springactionscript.org/schema/objects"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
        http://www.springactionscript.org/schema/objects
        http://www.springactionscript.org/schema/objects/spring-actionscript-objects-1.0.xsd">

    <!-- CE Console Extensions -->

    <object id="distributionContext" class="com.atricore.idbus.console.main.DistributionContext">
        <property name="distribution" value="josso-ce"/>
    </object>

    <!-- Branding Factory -->
    <object id="brandingFactory" class="com.atricore.idbus.console.branding.ce.AtricoreCEBrandingFactory"/>

    <!-- Palette Provider -->
    <object id="paletteProvider" class="com.atricore.idbus.console.branding.ce.CEPaletteItemProvider"/>

</objects>
